@model HomeModel
@{
    ViewData["Title"] = "Partnyor";
}

<!-- breadcrumb-area start -->
<div class="breadcrumb-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <ul class="breadcrumb-list">
                    <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Əsas səhifə</a></li>
                    <li class="breadcrumb-item active"><i class="fas fa-chevron-right"></i> Partnyorlarımız</li>
                </ul>
            </div>
        </div>
    </div>
</div>
<!-- breadcrumb-area end -->

<div class="partner-main">
    <div class="container">
        <div class="col-12">
            <div class="row justify-content-center">
                <div class="page-partner">
                    <div class="partner-title">Partnyorlarımız</div>
                    <div class="page-text">
                            <p>
                                <span>
                                    @foreach (var partner in Model.Partners)
                                    {
                                        <img src="~/img/@partner.Image" alt="" />
                                    }
                                </span>
                            </p>
                            
                        </div>
                </div>
            </div>
        </div>
    </div>
</div>

